  margin-top: 25rpx;
  margin-left: 15rpx;
  font-size: 30rpx;
  color: black;
  font-weight: bold;
}

.ars-address {
  margin-top: 15rpx;
  margin-bottom: 40rpx;
  font-size: 27rpx;
  margin-left: 10rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: rgb(65, 70, 72);
}

.goods-box {
  background: #fff;
  margin-top: -13rpx;
}

.goods-wrap {
  margin: 30rpx;
}

.labName {
  font-size: 30rpx;
  color: black;
  margin-top: 25rpx;
  margin-left: -10rpx;
}

.goods-img {
  margin: 20rpx;
  margin-left: 0;
  width: 200rpx;
  height: 250rpx;
}

.goods-name {
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 40rpx;
  color: black;
}

.goods-price {
  margin-top: 40rpx;
  color: orangered;
  font-size: 100%;
}

.goods-num {
  margin-top: 45rpx;
  margin-left: 40rpx;
  color: grey;
}

.goods-sumP {
  background-color: white;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 50rpx;
  font-size: 30rpx;
}

.goods-sum {
  margin-right: 30rpx;
  color: orangered;
  font-size: 120%;
  font-weight: bold;
}

.hidden {
  flex-grow: 3;
}

.deletebtn {
  margin-left: 300rpx;
  margin-top: -81rpx;
  color: black;
  background: white;
}

.paybtn {
  margin-right: 30rpx;
  color: white;
  background-color: orangered;
}

.buttom {
  position: fixed;
  bottom: 0;
  right: 0;
  z-index: 1;
  width: 100%;
  background: white;
}

.buttom button {
  width: 180rpx;
  height: 65rpx;
  font-size: 80%;
  margin-bottom: 20rpx;
}

.btmbtn {
  position: relative;
}

